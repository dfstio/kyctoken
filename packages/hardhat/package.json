{
  "name": "@kyctoken/hardhat",
  "version": "1.0.0",
  "description": "KYC token - typescript with hardhat",
  "main": "typechain-types/index.ts",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.1",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-interface-generator": "^0.0.6",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solidity-coverage": "^0.7.21",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@kyctoken/config": "1.0.0",
    "@openzeppelin/contracts": "^4.7.2",
    "fs": "^0.0.1-security"
  },
  "scripts": {
    "balance": "hardhat balance --network l3 0x07171714bE2b2e12B4F38270d446fB8bc82bb767",
    "archive": "git archive -o list-contracts.zip HEAD",
    "compile": "hardhat compile",
    "style": "prettier --write contracts/*.sol && prettier --write scripts/*.ts",
    "deploy": "hardhat run --network mumbai scripts/deploy.ts",
    "deploykyc": "hardhat run --network mumbai scripts/deployktoken.ts",
    "uniswap": "hardhat run --network mumbai scripts/uniswap.js",
    "accounts": "hardhat accounts",
    "verify": "hardhat verify --network mumbai 0x66D0656D8F1A4C8477B68394A7F31A41d35cf4EA 1691371557",
    "test": "hardhat test --network hardhat --grep hardhat",
    "testl3": "hardhat test --network l3 --grep L3",
    "size": "hardhat size-contracts",
    "abi": "hardhat export-abi",
    "interface": "yarn hardhat gen-interface contractname"
  }
}
